{

    "version": "1.0.4",
    "license": "MIT",
    "name": "ifresh/package-health",
    "type": "library",
    "description": "Sends updatable packages to the mothership",
    "autoload": {
        "psr-4": {
            "IFresh\\PackageHealth\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Stephan Bouman",
            "email": "s.bouman@ifresh.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.28|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "enlightn/security-checker": "^1.11"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IFresh\\PackageHealth\\HealthServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.7|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.5",
        "laravel/pint": "^1.27"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
